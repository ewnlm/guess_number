#!/usr/bin/perl -w
  use strict;
  my @golden;
  open FP, "golden.txt";
  while(<FP>) {
    chomp($_);
    push @golden, $_;
  }
  close FP;
  print "Golden: @golden\n";
  ### Test pattern
  my @default_pattern = (1234,4567,7890);
  my @target = split //, $golden[0];
  my @pattern = split //, $default_pattern[0];
  print "@target\n";
  print "@pattern\n";
